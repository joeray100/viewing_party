<%= form_with url: '/login', method: :post, local: true do |f| %>
  <%= f.label :user_name, "User Name: " %>
  <%= f.text_field :user_name %>

  <%= f.label :email, "Email: " %>
  <%= f.text_field :email %>

  <%= f.label :password, "password: " %>
  <%= f.text_field :password %>

  <%= f.submit 'Log in' %>
<% end %>
